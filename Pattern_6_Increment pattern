class Main {
    public static void main(String[] args) {
        int n=6;
        int count=1;
        for(int i=0;i<=n-1;i++){
            for(int j=0;j<=i;j++){
                System.out.print(count);
                count++;
            }
            System.out.println();            
        }    
    }
}
