class Solution {
    public boolean isPalindrome(int n) {
        int temp =n;
        int sum=0;
        while(n>0){
            int re = n%10;
            sum =10*sum+(re);
            n=n/10;
        }
        if(temp==sum){
            return true;
        }
        else{
            return false;
        }
}
}
