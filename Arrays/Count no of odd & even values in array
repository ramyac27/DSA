class Main {
    public static void main(String[] args) {
        int[] x = {1,2,3,4,5};
        int count_even=0;
        int count_odd=0;
        for(int i=0;i<x.length;i++){
            if(x[i]%2==0){
                count_even++;
            }
            else{
                count_odd++;
            }
        }
        System.out.println(count_even);
        System.out.println(count_odd);
        
    }
}
