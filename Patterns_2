Input:

n = 4

Expected output:

4321

321

21

1



import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
		int x = scanner.nextInt();
		for(int i=0;i<x;i++) {
			for(int j=x;j>i;j--) {
				System.out.print(j-i);
			}
			System.out.println();
		}
    }
}
